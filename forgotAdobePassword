const forgotAdobePassword = (userName, userInput) => {
  MailApp.sendEmail({
    to: "",
    subject: " |＾◡＾| Can you help out " + userName + " w/reseting adobe password",
    htmlBody: userName +" Said this to UVA_Bot " + userInput
  });
  return {
    "cards": [
      {
        "header": {
          "title": "| ＾◡＾| Sending ticket to Daniel",
          "subtitle": "look for a response in your gmail soon"
        }
      }
    ]
  }
}
